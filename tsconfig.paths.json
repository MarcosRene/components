{
  "compilerOptions": {
    "baseUrl": "src",
    "paths": {
      "@assets": [
        "assets"
      ],
      "@assets/*": [
        "assets/*"
      ],
      "@components": [
        "components"
      ],
      "@components/*": [
        "components/*"
      ],
      "@constants": [
        "constants"
      ],
      "@constants/*": [
        "constants/*"
      ],
      "@contexts": [
        "contexts"
      ],
      "@contexts/*": [
        "contexts/*"
      ],
      "@hooks": [
        "hooks"
      ],
      "@hooks/*": [
        "hooks/*"
      ],
      "@pages": [
        "pages"
      ],
      "@pages/*": [
        "pages/*"
      ],
      "@routes": [
        "routes"
      ],
      "@routes/*": [
        "routes/*"
      ],
      "@services": [
        "services"
      ],
      "@services/*": [
        "services/*"
      ]
